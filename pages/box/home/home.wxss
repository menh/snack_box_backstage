page {
  width: 100%;
}
.page {
  width: 100%;
  background-color:#f4f4f4;
}

.page-title {
  height: 310rpx;
}

.page-content-item-property {
  width: 230rpx;
  margin-left: 20rpx;
  line-height: 100%;
  display: flex;
  flex-direction: column;
  align-items: left;
}

.page-content-item-property-content {
  margin-bottom: 6rpx;
  color: #808080;
  font-size: 25rpx;
}

.page-content-item-data {
  display: flex;
  flex-direction: column;
}

.page-content-item-data-colum {
  display: flex;
  flex-direction: row;
}

.page-content-item-data-colum-item {
  width: 105rpx;
  height: 85rpx;
  line-height: 100%;
  margin-right: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.page-content-item-data-colum-item-data {
  width: 100%;
  height: 55rpx;
  line-height: 55rpx;
  font-size: 25rpx;
  text-align: center;
}

.page-content-item-data-colum-item-title {
  width: 100%;
  height: 25rpx;
  line-height: 25rpx;
  font-size: 22rpx;
  font-weight: 300;
  color: #808080;
  text-align: center;
}

.page-content-item-data-colum-item-data-day {
  height: 25rpx;
  font-size: 22rpx;
  line-height: 100%;
}

.page-content-item-data-colum-item-data-time {
  height: 25rpx;
  font-size: 22rpx;
  line-height: 100%;
}
